<h1>Ipassets</h1>
<% if !@ipasset.errors.empty? %>
   <div class = "alert alert-error">
      
      <ul>
         <% @ipasset.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
         <% end %>
      </ul>
      
   </div>
<% end %>

<div class = "well">
   <%= form_for @ipasset, html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.label :Description %>
      <%= f.text_area :Description %>
      
      <%= f.submit "Add Owner",  class: "btn btn-primary" %>
   <% end %>
</div>

<%= link_to 'Back', ipassets_index_path %>
